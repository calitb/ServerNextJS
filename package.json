{
  "name": "calitb-dev",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:inspect": "NODE_OPTIONS='--inspect' next dev",
    "build": "./scripts/build.sh && next build",
    "start": "next start",
    "build:start": "yarn build && yarn start",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "test": "jest",
    "test:dev": "jest --watch",
    "test:ci": "jest --coverage",
    "cy": "cypress open",
    "cy:run": "cypress run",
    "wait-fe": "wait-on http://localhost:3000",
    "e2e": "NODE_ENV=e2e start-server-and-test build:start http://localhost:3000 cy:run",
    "test:coverage": "rm -rf .nyc_output cypress-coverage jest-coverage coverage && yarn test:ci && yarn e2e && yarn merge:reports",
    "merge:reports": "node ./scripts/mergeCoverage.js",
    "test:coverage:report": "npx nyc report --reporter=text",
    "test:coverage:percentage": "sed -n 47p coverage/lcov-report/index.html | awk -F '>' '{print $2}' | awk -F '%' '{print $1}'"
  },
  "dependencies": {
    "@types/jsdom": "^12.2.3",
    "apollo-server-micro": "^2.19.2",
    "autoprefixer": "^10.2.4",
    "contentful": "^8.1.7",
    "graphql": "^15.5.0",
    "jsdom": "^11.10.0",
    "next": "10.0.6",
    "postcss": "^8.2.4",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "request": "^2.85.0",
    "tailwindcss": "^2.0.2"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.2",
    "@cypress/instrument-cra": "^1.4.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "apollo": "^2.32.1",
    "apollo-boost": "^0.4.9",
    "apollo-cache-inmemory": "^1.6.6",
    "babel-jest": "^26.6.3",
    "babel-plugin-istanbul": "^6.0.0",
    "contentful-cli": "^1.5.29",
    "contentful-management": "^7.8.0",
    "contentful-migration": "^4.0.7",
    "cross-fetch": "^3.0.6",
    "cypress": "^6.5.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-react": "^7.22.0",
    "istanbul-lib-coverage": "^3.0.0",
    "jest": "^26.6.3",
    "jest-watch-typeahead": "^0.6.1",
    "nyc": "^15.1.0",
    "sitemap": "^6.4.0",
    "start-server-and-test": "^1.12.0",
    "typescript": "^4.1.3",
    "wait-on": "^5.2.1"
  },
  "nyc": {
    "report-dir": "cypress-coverage"
  }
}
